	&ck_i2c0 {
		status = "ok";
		rt5036@38 {
			compatible = "rt,rt5036";
			reg = <0x38>;
			rt5036_dcdc1: regulator_0 {
				compatible = "rt,rt5036-dcdc1";
				cell-index = <0>;
				regulator-name = "rt5036-dcdc1";
				regulator-min-microvolt = < 800000>;
				regulator-max-microvolt = <3300000>;
				rt,nramp_sel = <0x00>;
				rt,sramp_sel = <0x00>;
				rt,allow_mode_mask;
				//rt,standby_vol = <xxx>; unit:uV
				rt,standby_enabled;
				//rt,standby_disabled;
				qcom,consumer-supplies = "rt5036-dcdc1", "";
			};
			rt5036_dcdc2: regulator_1 {
				compatible = "rt,rt5036-dcdc2";
				cell-index = <1>;
				regulator-name = "rt5036-dcdc2";
				regulator-min-microvolt = < 800000>;
				regulator-max-microvolt = <3300000>;
				rt,nramp_sel = <0x00>;
