			rt5036-charger {
				compatible = "rt,rt5036-charger";
				rt,te_en;
				rt,iprec = <0x2>;
				rt,ieoc = <0x3>;
				rt,vprec = <0xA>;
				rt,batlv = <0x4>;
				rt,vrechg = <1>;
				rt,chg_volt = <4200>;
				rt,otg_volt = <5025>;
				rt,acchg_icc = <2000>;
				rt,usbtachg_icc = <2000>;
				rt,usbchg_icc = <900>;/*
				rt,acdet_gpio = <&msmgpio 59 0x00>;
				rt,usbdet_gpio = <&msmgpio 65 0x00>;*/
			};
			rt5036-rtc {
				compatible = "rt,rt5036-rtc";
			};
			rt5036-misc {
				compatible = "rt,rt5036-misc";
				rt,shdn_press = <0x1>;
				rt,stb_en = <0>;
				rt,lp_enshdn;
				rt,vsysuvlo = <0x2>;
				rt,syslv_enshdn;
			};
			rt5036-irq {
				compatible = "rt,rt5036-irq";
				rt,irq_gpio = <&ck_gpa0 5 GPIO_ACTIVE_HIGH>;
			};
			rt5036-debug {
				compatible = "rt,rt5036-debug";
			};
		};
	};
