		uart0: serial0@0x02309000 {
			compatible = "snps,dw-apb-uart";
			reg = <0x02309000 0x1000>;
			interrupt-parent = <&intc>;
			interrupts = <15>;
			reg-shift = <2>;
			reg-io-width = <4>;
			clocks = <&apb2_uart>;
		};

		uart1: serial1@0x02308000 {
			compatible = "snps,dw-apb-uart";
			reg = <0x02308000 0x1000>;
			interrupt-parent = <&intc>;
			interrupts = <14>;
			reg-shift = <2>;
			reg-io-width = <4>;
			cts-override;
			clocks = <&apb2_uart>;
		};

