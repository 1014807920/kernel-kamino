		i2c1: i2c@02203000 {
			#address-cells = <1>;
			#size-cells = <0>;
			compatible = "snps,designware-i2c";
			reg = <0x02203000 0x1000>;
			clocks = <&apb0>;
			interrupts = <2>;
			status = "okay";
		};

		ck_i2c0: i2c@0205000 {
			#address-cells = <1>;
			#size-cells = <0>;
			compatible = "snps,designware-i2c";
			reg = <0x0205000 0x1000>;
			clocks = <&apb0>;
			interrupts = <23>;
			status = "disable";
		};

		ck_i2c1: i2c@0206000 {
			#address-cells = <1>;
